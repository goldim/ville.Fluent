{
  "$schema": "https://qooxdoo.org/schema/Manifest-2-0-0.json",
  "info": {
    "name": "ville.Fluent",
    "summary": "a theme for Qooxdoo desktop applications inspired by Microsofts Fluent UI",
    "description": "A theme inspired by Microsofts Fluent UI",
    "homepage": "http://github.com/sqville/ville.Fluent",
    "license": "MIT",
    "authors": [
      {
        "name": "Chris Eskew",
        "email": "sqville@gmail.com"
      }
    ],
    "version": "1.0.0"
  },
  "provides": {
    "namespace": "ville.theme.Fluent",
    "encoding": "utf-8",
    "class": "source/class",
    "resource": "source/resource",
    "translation": "source/translation",
    "application": {
      "class": "qxl.widgetbrowser.Application",
      "theme": "ville.theme.Fluent",
      "name": "widgetbrowser",
      "title": "WidgetBrowser",
      "include": [
        "qx.theme.Simple",
        "qx.theme.Classic",
        "qxl.widgetbrowser.pages.Tree",
        "qxl.widgetbrowser.pages.List",
        "qxl.widgetbrowser.pages.Table",
        "qxl.widgetbrowser.pages.Form",
        "qxl.widgetbrowser.pages.ToolBar",
        "qxl.widgetbrowser.pages.Window",
        "qxl.widgetbrowser.pages.Tab",
        "qxl.widgetbrowser.pages.Control",
        "qxl.widgetbrowser.pages.Embed",
        "qxl.widgetbrowser.pages.EmbedFrame",
        "qxl.widgetbrowser.pages.Basic",
        "qxl.widgetbrowser.pages.Misc"
      ],
      "exclude": [
        "qx.theme.Modern"
      ]
    }
  },
  "requires": {
    "@qooxdoo/framework": "^7.5.0",
    "qooxdoo/qxl.formdemo": "^2.0.0",
    "qooxdoo/qxl.versionlabel": "^2.0.0"
  }
}